{
  "name": "orchitech-notes-api",
  "version": "1.0.0",
  "description": "ukazka prace",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run serve",
    "serve": "node -r dotenv/config build/server.js",
    "build": "npm run build-ts",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-node": "nodemon -r dotenv/config build/server.js",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json --fix",
    "jest-clear": "jest --clearCache",
    "test": "jest --forceExit --detectOpenHandles --verbose",
    "test:file": "npm run jest-clear && jest --forceExit --detectOpenHandles --verbose"
  },
  "author": "klapka.ota@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "lodash": "^4.17.15",
    "mongoose": "^5.9.13",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/hapi__joi": "^17.1.0",
    "@types/helmet": "0.0.46",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.150",
    "@types/mongoose": "^5.7.16",
    "@types/node": "^13.13.5",
    "@types/supertest": "^2.0.9",
    "colors": "^1.4.0",
    "concurrently": "^5.2.0",
    "dotenv": "^8.2.0",
    "jest": "^26.0.1",
    "nodemon": "^2.0.3",
    "supertest": "^4.0.2",
    "ts-jest": "^25.5.1",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.2",
    "typescript": "^3.8.3"
  }
}
